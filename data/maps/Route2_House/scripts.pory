    walk_fast_left
    face_up
}

movement AkiShinjiWakesUp {
    set_visible
    jump_in_place_up
}

movement AkihikoLookingAround {
    face_left
    delay_16
    delay_16
    emote_pensive
    delay_16
    face_right
    delay_16
    delay_16
    delay_16
    face_up
    
}

