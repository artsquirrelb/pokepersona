script Route2_House_Eventscript_Mitsuru {
    lock
    faceplayer
    if (var(VAR_AKIHIKOS_INTRO_STATE) == 29){
        msgbox(format("{SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_NORMAL}Have you both finished talking?\p
        {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}Not yet. We still need more time.\p
        {SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_SMILE}Be my guest."))
        closemessage
    }
    elif(var(VAR_AKIHIKOS_INTRO_STATE) == 30){
        setflag(FLAG_HIDE_HELP_BUTTON)
        msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}I also have a request.\p
        {SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_SMILE}Certainly, let's hear it."))
        closemessage
        setvar(VAR_AKIHIKOS_INTRO_STATE, 31)
        setflag(FLAG_DONT_TRANSITION_MUSIC)
        warpsilent(MAP_BLANK_MAP, 0, 0)
    }
    release
}

script Route2_House_Eventscript_Shinjiro {
    lock
    faceplayer
    msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}What's wrong, Shinji?\p
    {SPEAKER NAME_S}{CREATE_MUGSHOT MUGSHOT_S EMOTE_SERIOUS}I don't trust her.\p
    She's hiding lots of information and we don't even know if it's true that she's not on those men's side.\p
    {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_SERIOUS}But she's also the only lead we have right now. We didn't even know where to start looking for Miki.\p
    {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_CONFIDENT}Besides, she said it'd be a great challenge.\p
    {SPEAKER NAME_S}{CREATE_MUGSHOT MUGSHOT_S EMOTE_NORMAL}So you're tempted.\p
    {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_SERIOUS}Honestly, yes. \p
    If what she said was true, I may find Miki or at least her whereabouts while going after those men in black.\p
    {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}I'm joining her. Don't stop me.\p
    {SPEAKER NAME_S}{CREATE_MUGSHOT MUGSHOT_S EMOTE_SERIOUS}Told you I didn't trust her.\p
    So if you join her, I will too. Someone's gotta keep an eye on her.\p
    {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_CONFIDENT}Alright, let's try asking her then."))
    closemessage
    applymovement(6, ShinjiroJoinsAndVanish)
    waitmovement(0)
    setvar(VAR_AKIHIKOS_INTRO_STATE, 30)
    release
    end
}

movement ShinjiroJoinsAndVanish {
    walk_down
    set_invisible
    walk_fast_right
    walk_fast_up *2
}

