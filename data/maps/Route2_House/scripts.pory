    MAP_SCRIPT_ON_LOAD{
        if (var(VAR_MITSURUS_INTRO_STATE) == 22){
            setmetatile(2, 10, 0x24B, TRUE)
            setmetatile(2, 11, 0x253, TRUE)
        }
    }
    MAP_SCRIPT_ON_WARP_INTO_MAP_TABLE[
        VAR_AKIHIKOS_INTRO_STATE, 27: Route2_House_Eventscript_AkihikoOnWarp27
        VAR_AKIHIKOS_INTRO_STATE, 32: Route2_House_Eventscript_AkihikoOnWarp32
