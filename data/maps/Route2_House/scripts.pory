script Route2_House_Eventscript_Bathroom {
    lock
    if (var(VAR_TEMP_0) == 0){
        msgbox(format("Do you want to use the bathroom?"), MSGBOX_YESNO)
        closemessage
        if (var(VAR_RESULT) == TRUE){
            hidefollowernpc
            fadescreenswapbuffers(FADE_TO_BLACK)
            destroyfollowernpc
            clearflag(FLAG_FOLLOWERS_DISABLED)
            playse(SE_DOOR)
            waitse
            playse(SE_M_SURF)
            waitse
            delay(30)
            playse(SE_DOOR)
            waitse
            applymovement(LOCALID_PLAYER, faceleft)
            waitmovement(0)
            delay (30)
            fadescreenswapbuffers(FADE_FROM_BLACK)
            delay(20)
            playse(SE_SHINY)
            applymovement(LOCALID_PLAYER, emotemusic)
            waitmovement(0)
            checkplayergender
            if (var(VAR_RESULT) == MALE){
                addtime(0, 0, 10)
                msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_HAPPY}(All refreshed.)"))
            }
            else{
                addtime(0, 0, 40)
                msgbox(format("{SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_SMILE}(That really freshened me up.)"))
            }
            closemessage
            special(HealPlayerParty)
            setvar(VAR_TEMP_0, 1)
        }
        else{
            checkplayergender
            if(var(VAR_RESULT) == MALE){
                msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}(Nah, I think I'm good.)"))
            }
            else{
                msgbox(format("{SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_NORMAL}(Let's do it later.)"))
            }
            closemessage
            applymovement(LOCALID_PLAYER, faceleft)
            waitmovement(0)
        }
    }
    elif (var(VAR_TEMP_0) == 1){
        checkplayergender
        if (var(VAR_RESULT) == MALE){
            msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}(I just used the bathroom.)"))
        }
        else{
            msgbox(format("{SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_NORMAL}(Let's not waste time any further.)"))
        }
        closemessage
        applymovement(LOCALID_PLAYER, faceleft)
        waitmovement(0)
        checkplayergender
        if(var(VAR_RESULT) == MALE && var(VAR_AKIHIKOS_INTRO_STATE) >= 42)
        {
            setvar(VAR_TEMP_0, 2)
        }
        release
        end

    }
    elif(var(VAR_TEMP_0) == 2){
        msgbox(format("{SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_NORMAL}(That reminds me, Kirijo took a really long bath yesterday.)\p
        {SPEAKER NAME_A}{CREATE_MUGSHOT MUGSHOT_A EMOTE_DOUBT}I don't understand. What's so fun in the bathroom that made her stay that long?\p
        {SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_NORMAL}What are you staring at the bathroom door for? Is there anything wrong?\p
