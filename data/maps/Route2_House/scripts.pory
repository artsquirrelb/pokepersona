script Route2_House_Eventscript_TheFirstMorning2 {
    delay(30)
    msgbox(format("{SPEAKER NAME_M}{CREATE_MUGSHOT MUGSHOT_M EMOTE_NORMAL}I believe our destination is around the base of Goldenwings Mountain.\p
