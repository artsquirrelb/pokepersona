    additem(ITEM_POKE_BALL, VAR_0x800A)
    additem(ITEM_GREAT_BALL, VAR_0x800B)
    additem(ITEM_EXP_CANDY_XS, 20)
    setvar(VAR_0x800A, 0)
    setvar(VAR_0x800B, 0)
