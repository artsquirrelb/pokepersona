        VAR_MITSURUS_INTRO_STATE, 32: Route2_House_Eventscript_MitsuruOnFrame32
        VAR_MITSURUS_INTRO_STATE, 33: Route2_House_Eventscript_MitsuruOnFrame33
        VAR_MITSURUS_INTRO_STATE, 34: Route2_House_Eventscript_MitsuruOnFrame34
        ///////
        VAR_WINDWARD_HARBOR_GHOST_BOY, 44: Route2_House_Eventscript_GhostBoyMadPostQuest
    ]
