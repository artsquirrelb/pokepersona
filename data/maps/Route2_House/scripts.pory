        VAR_MITSURUS_INTRO_STATE, 28: Route2_House_Eventscript_MitsuruOnWarp28
        
    ]
    MAP_SCRIPT_ON_TRANSITION{
        setrespawn(HEAL_LOCATION_ROUTE2_HOUSE)
        if (var(VAR_AKIHIKOS_INTRO_STATE) != 39){
            setflag(FLAG_TEMP_3)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 27){
            setflag(FLAG_SPAWN_INVISIBLE)
            setobjectxyperm(6, 6, 8)
            playbgm(MUS_NONE, TRUE)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 29){
            clearflag(FLAG_HIDE_HELP_BUTTON)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) >= 29 && var(VAR_MITSURUS_INTRO_STATE) != 28){
            setflag(FLAG_TEMP_2)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 32){
            setobjectxyperm(6, 6, 7)
            setobjectxyperm(5, 1, 8)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) > 32 && var(VAR_AKIHIKOS_INTRO_STATE) < 38){
            setflag(FLAG_TEMP_1)
            setflag(FLAG_TEMP_4)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 38){
            setflag(FLAG_HIDE_HELP_BUTTON)
            setobjectxyperm(6, 6, 7)
            setobjectmovementtype(1, MOVEMENT_TYPE_FACE_RIGHT)
            setobjectmovementtype(5, MOVEMENT_TYPE_WALK_IN_PLACE_LEFT)
            setobjectxyperm(4, 8, 4)
            setobjectmovementtype(4, MOVEMENT_TYPE_FACE_DOWN)
            playbgm(MUS_NONE, TRUE)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 39){
            setflag(FLAG_HIDE_HELP_BUTTON)
            setflag(FLAG_TEMP_1)
            setobjectxyperm(5, 4, 4)
            setobjectmovementtype(4, MOVEMENT_TYPE_FACE_UP)
            setobjectxyperm(4, 4, 7)
            setobjectmovementtype(5, MOVEMENT_TYPE_FACE_LEFT)
            setobjectxyperm(6, 11, 6)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) == 42){
            setobjectxyperm(6, 8, 8)
            setobjectmovementtype(6, MOVEMENT_TYPE_FACE_RIGHT)
        }
        if (var(VAR_AKIHIKOS_INTRO_STATE) >= 43 && var(VAR_MITSURUS_INTRO_STATE) != 22 
        && var(VAR_MITSURUS_INTRO_STATE) != 26 && var(VAR_MITSURUS_INTRO_STATE) != 28
        && var(VAR_MITSURUS_INTRO_STATE) != 32 && var(VAR_MITSURUS_INTRO_STATE) != 33
        && var(VAR_MITSURUS_INTRO_STATE) != 34){
            setflag(FLAG_TEMP_1)
            setflag(FLAG_TEMP_4)
        }
        if(var(VAR_MITSURUS_INTRO_STATE) == 22){
            setflag(FLAG_TEMP_1)
            setobjectxyperm(5, 2, 9)
            setobjectmovementtype(5, MOVEMENT_TYPE_WALK_IN_PLACE_DOWN)
            setobjectxyperm(4, 7, 13)
            setobjectmovementtype (4, MOVEMENT_TYPE_FACE_UP)
        }
        if(var(VAR_MITSURUS_INTRO_STATE) == 26){
            setflag(FLAG_TEMP_1)
            setobjectxyperm(4, 7, 4)
            setobjectmovementtype(4, MOVEMENT_TYPE_FACE_DOWN)
            setobjectxyperm(5, 4, 4)
            setobjectmovementtype(5, MOVEMENT_TYPE_WALK_IN_PLACE_UP)
        }
        if(var(VAR_MITSURUS_INTRO_STATE) != 32){
            setflag(FLAG_TEMP_5)
        }
        if(var(VAR_MITSURUS_INTRO_STATE) == 28)
        {
            setflag(FLAG_SPAWN_INVISIBLE)
            setobjectxyperm(6, 6, 8)
        }
        if(var(VAR_MITSURUS_INTRO_STATE) == 32){
            setflag(FLAG_SPAWN_INVISIBLE)
            setflag(FLAG_HIDE_HELP_BUTTON)
            setobjectxyperm(6, 6, 7)
            setobjectxyperm(8, 5, 7)
            setobjectmovementtype(1, MOVEMENT_TYPE_FACE_RIGHT)
            setobjectmovementtype(5, MOVEMENT_TYPE_WALK_IN_PLACE_LEFT)
            setobjectxyperm(4, 8, 4)
            setobjectmovementtype(4, MOVEMENT_TYPE_FACE_DOWN)
            playbgm(MUS_NONE, TRUE)
        }
        if (var(VAR_MITSURUS_INTRO_STATE) == 33)
        {
            setflag(FLAG_SPAWN_INVISIBLE)
            setobjectxyperm(6, 8, 8)
            setobjectxyperm(8, 3, 11)
            setobjectmovementtype(8, MOVEMENT_TYPE_FACE_LEFT)
        }
        if (var(VAR_MITSURUS_INTRO_STATE) == 34)
        {
            setflag(FLAG_HIDE_HELP_BUTTON)
            setflag(FLAG_SPAWN_INVISIBLE)
            setobjectxyperm(6, 6, 7)
            setobjectmovementtype(6, MOVEMENT_TYPE_FACE_UP)
            setobjectxyperm(8, 5, 7)
            setobjectmovementtype(8, MOVEMENT_TYPE_FACE_UP)
        }
    }

    MAP_SCRIPT_ON_FRAME_TABLE[
        VAR_AKIHIKOS_INTRO_STATE, 27: Route2_House_Eventscript_AkihikoOnFrame27
        VAR_AKIHIKOS_INTRO_STATE, 32: Route2_House_Eventscript_AkihikoOnFrame32
        VAR_AKIHIKOS_INTRO_STATE, 38: Route2_House_Eventscript_AkihikoOnFrame38
        VAR_AKIHIKOS_INTRO_STATE, 39: Route2_House_Eventscript_AkihikoOnFrame39
        VAR_AKIHIKOS_INTRO_STATE, 42: Route2_House_Eventscript_AkihikoOnFrame42
        VAR_AKIHIKOS_INTRO_STATE, 43: Route2_House_Eventscript_AkihikoOnFrame43
