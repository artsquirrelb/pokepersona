script Route2_House_Eventscript_AkiShinjiDecidingToJoinMitsuru {
    lock
    delay(30)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}So what's your request?\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_NORMAL}Let me join too."))
    closemessage
    applymovement(1, MitsuruLookingAtShinjiro)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}Hm... Shinjiro Aragaki-kun, correct?\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}... You sure have done your research thoroughly.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}I'll take that as a compliment.\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}That wasn't.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}Heh...\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}I have yet to know if you're able to hold your ground when situation calls.\p
    If I'm not mistaken, you don't have a Pokémon of your own yet.\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}I can just catch one.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}Alright. Then how about this.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_CONFIDENT}Before the sun sets, if you can beat me in a Pokémon battle, you can come with us."))
    closemessage
    applymovement(1, walkdown)
    waitmessage
    msgbox(format("Mitsuru gave Shinjiro a modern device, some Pokéballs and Exp Candies."))
    applymovement(1, stepbackup)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}It wouldn't be fair if you have nothing to catch a Pokémon.\p
    Oh, and that device is a Pokédex. I'm just lending it to you.\p
    It's yours if you can beat me.\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_CONFIDENT}Hmph. Just you wait."))
    closemessage
    applymovement(1, MitsuruGivingAkihikoPokedex)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}And these are for you."))
    closemessage
    playse(MUS_OBTAIN_ITEM)
    waitse
    msgbox(format("Akihiko received a Pokédex and a wooden box.\p
    Inside was a good bunch of Pokéballs, Exp Candies and some essential Key Items."))
    closemessage
    additem(ITEM_POKE_BALL, 20)
    additem(ITEM_EXP_CANDY_XS, 20)
    additem(ITEM_EXP_ALL)
    setflag(FLAG_SYS_POKEDEX_GET)
    setflag(FLAG_SYS_NATIONAL_DEX)
    special(EnableNationalPokedex)
    setcaughtmon(SPECIES_PAWMI)
    setcaughtmon(SPECIES_PAWMI_DELTA)
    applymovement(1, stepbackup)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_CONFIDENT}To be honest, I'd also like to have a battle with you, Sanada-kun.\p
    It's better to personally see how you fare in battle.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_CONFIDENT}Heh, sure."))
    closemessage
    applymovement(1, MitsuruGoesBackToCenter)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_CONFIDENT}Alright. I'll be waiting for both of you in Sunshine Meadow.\p
    We'll be having 3-vs-3 Pokémon battles so plan your party accordingly.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}Good luck, you'll need it."))
    closemessage
    special(SpawnCameraObject)
    applymovement(OBJ_EVENT_ID_CAMERA, CameraPansToLowerHalfOfRoom)
    applymovement(1, MitsuruExitsTheHouse)
    applymovement(LOCALID_PLAYER, faceright)
    waitmovement(LOCALID_PLAYER)
    applymovement(6, faceleft)
    waitmovement(6)
    delay(16)
    applymovement(5, VulpixFollowsMitsuruExit)
    delay(4)
    applymovement(4, InteleonFollowsMitsuruExit)
    applymovement(LOCALID_PLAYER, facedown)
    applymovement(6, facedown)
    waitmovement(1)
    playse(SE_EXIT)
    waitse
    waitmovement(5)
    playse(SE_EXIT)
    waitse
    waitmovement(4)
    playse(SE_EXIT)
    applymovement(OBJ_EVENT_ID_CAMERA, CameraFromLowerHalfRoomBackToAkihiko)
    waitmovement(0)
    delay(30)
    msgbox(format("{SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_NORMAL}Bossy.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_CONFIDENT}Heh. She's one weird rich girl.\p
