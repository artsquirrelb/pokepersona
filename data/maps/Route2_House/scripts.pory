    applymovement(6, invisible)    
}

script Route2_House_Eventscript_AkihikoOnWarp32 {
    applymovement(LOCALID_PLAYER, faceup)
    applymovement(6, faceup)
    applymovement(5, faceright)
}

script Route2_House_Eventscript_AkihikoOnWarp38 {
    applymovement(6, faceup)
}
script Route2_House_Eventscript_AkihikoOnWarp39 {
    applymovement(6, invisible)
    applymovement(LOCALID_PLAYER, faceleft)
}
script Route2_House_Eventscript_AkihikoOnWarp43 {
    applymovement(LOCALID_PLAYER, faceleft)
}


script Route2_House_Eventscript_AkihikoOnFrame27 {
    goto (Route2_House_Eventscript_AkiShinjiWakingUp)
}

script Route2_House_Eventscript_AkihikoOnFrame32 {
    goto (Route2_House_Eventscript_AkiShinjiDecidingToJoinMitsuru)
}

script Route2_House_Eventscript_AkiShinjiWakingUp {
    lock
    delay(20)
    applymovement(LOCALID_PLAYER, CameraPansDownToTheTrio)
    waitmovement(0)
    delay(30)
    setspeaker("???????")
    msgbox(format("{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}..."))
    closemessage
    delay(30)
    applymovement(2, shakehorizontal)
    delay(4)
    applymovement(3,shakehorizontal)
    waitmovement(0)
    delay(20)
    special(SpawnCameraObject)
    applymovement(LOCALID_PLAYER, BackToSprite)
    waitmovement(0)
    applymovement(2, invisible)
    clearflag(FLAG_SPAWN_INVISIBLE)
    playse(SE_PIN)
    applymovement(LOCALID_PLAYER, AkiShinjiWakesUp)
    waitmovement
    delay(4)
    applymovement(3, invisible)
    playse(SE_PIN)
    applymovement(6, AkiShinjiWakesUp)
    waitmovement(0)
    delay(30)
    applymovement(LOCALID_PLAYER, AkihikoLookingAround)
    delay(8)
    applymovement(6, ShinjiroLookingAround)
    waitmovement(0)
    delay(30)
    applymovement(LOCALID_PLAYER, jumpinplaceup)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}Wh-Where are we?\p
    And who are you??"))
    setspeaker("???????")
    msgbox(format("{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}Apologies for this is the best shelter I could provide in this situation.\p
    This house is of my butler's grandson, who's currently traveling the region.\p
    We're still in Route 2. Worry not, it's safe here."))
    closemessage
    goto(Route2_House_Eventscript_TriosFirstGreeting)
    release
}

script Route2_House_Eventscript_TriosFirstGreeting {
    lock
    delay(30)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}As for my name, I am Mitsuru Kirijo.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}-!\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SURPRISED}Kirijo-?!"))
    closemessage
    delay(30)
    applymovement(LOCALID_PLAYER, faceright)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_DOUBT}Uhhh, who? The name sounds familiar."))
    closemessage
    applymovement(6, faceleft)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SURPRISED}-!\p
    Dumbass! That's the billionaire group who donated us our PCs!\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}Oooh..."))
    delay(20)
    applymovement(LOCALID_PLAYER, faceup)
    delay(8)
    applymovement(6, faceup)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}So? What does a billionaire girl like you want with us?\p
    For whatever it is, I'm not interested. I only want to train and become a stronger trainer.\p
    I could've dealed with the Drowzee earlier if I were-\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}Wait- Are you with the men in black who tried to kidnap us!?\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SAD}...\p 
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}No. \p
    In fact, I managed to disrupt them and brought you two here.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SERIOUS}To be more precise, my mission is to stop them.\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}Stop them?\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SERIOUS}Both of you are from Sunshine Orphanage, correct?\p
    Right now I can say for certain that the explosion which happened a few days ago was their doing.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}!!\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SURPRISED}-!"))
    closemessage
    delay(20)
    applymovement(1, faceleft)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SAD}(Mumble) If only I were able to stop them in time... It was my fault...\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}....."))
    closemessage
    applymovement(1, facedown)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}My request is rather simple.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}Request...?"))
    closemessage
    applymovement(1, MitsuruPickingAkihiko)
    waitmovement
    delay(8)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}I would like you to join me, Akihiko Sanada-kun."))
    closemessage
    playse(SE_PIN)
    waitse
    applymovement(LOCALID_PLAYER, emoteexclamation)
    applymovement(6, emoteexclamation)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SURPRISED}Huh?\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}I believe you have a reason for wanting to get stronger.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}.....\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}I have clues of where to find those men in black and what they are doing.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}And earlier I saw your battle with them. You have great potential.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}If you'll join me... I promise it'll be a great challenge.\p
    I'll also share what I know about those men with you."))
    closemessage
    applymovement(LOCALID_PLAYER, faceright)
    delay(16)
    applymovement(6, faceleft)
    waitmovement(0)
    delay(60)
    applymovement(LOCALID_PLAYER, faceup)
    delay(16)
    applymovement(6, faceup)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}.....\p
    {SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}.....\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_SERIOUS}Alright, I'll join you.\p
    {SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_SMILE}Very well."))
    closemessage
    playse(SE_PIN)
    waitse
    applymovement(6, ShinjiroSurprisedAtAkihikoChoice)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}Wait!"))
    closemessage
    applymovement(6, ShinjiroDisruptsAkiMitsu)
    applymovement(LOCALID_PLAYER, faceright)
    waitmovement(0)
    msgbox(format("{SPEAKER NAME_SHINJIRO}{CREATE_MUGSHOT MUGSHOT_SHINJIRO EMOTE_SERIOUS}Aki, we need to talk.\p
    {SPEAKER NAME_AKIHIKO}{CREATE_MUGSHOT MUGSHOT_AKIHIKO EMOTE_NORMAL}Shinji?"))
    closemessage
    applymovement(6, faceup)
    delay(8)
    applymovement(LOCALID_PLAYER, faceup)
    waitmovement(0)
    delay(60)
    msgbox(format("{SPEAKER NAME_MITSURU}{CREATE_MUGSHOT MUGSHOT_MITSURU EMOTE_NORMAL}Go ahead, I'll be waiting right here."))
    closemessage
    applymovement(1, MitsuruGoesBackToPlace)
    waitmovement(0)
    setvar(VAR_AKIHIKOS_INTRO_STATE, 28)
    warpsilent(MAP_BLANK_MAP, 0, 0)
    special(RemoveCameraObject)
    fwdtime(13, 20)
    pausefakertc
    release
    end
}

